{
	"name": "yoda",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "npm run db && svelte-kit build && npm run postbuild",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"db": "prisma generate",
		"db:mig": "prisma migrate dev --name",
		"db:push": "prisma db push",
		"db:dev": "prisma studio",
		"postinstall": "npm run db",
		"postbuild": "cp node_modules/@prisma/engines/libquery_engine-* .vercel_build_output/functions/node/render/; cp prisma/schema.prisma .vercel_build_output/functions/node/render/"
	},
	"devDependencies": {
		"@prisma/client": "^3.15.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "0.5.1",
		"@typescript-eslint/eslint-plugin": "5.28.0",
		"@typescript-eslint/parser": "5.28.0",
		"autoprefixer": "10.4.7",
		"carbon-icons-svelte": "11.1.0",
		"eslint": "8.17.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"gpt-3-encoder": "1.1.3",
		"openai": "3.0.0",
		"postcss": "8.4.14",
		"postcss-nesting": "10.1.8",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"prisma": "3.15.2",
		"svelte": "3.48.0",
		"svelte-check": "2.7.2",
		"svelte-loading-spinners": "0.1.7",
		"svelte-preprocess": "4.10.7",
		"tailwindcss": "3.1.3",
		"tslib": "2.4.0",
		"typescript": "4.7.3"
	},
	"type": "module",
	"dependencies": {
		"cookie": "0.5.0",
		"yup": "0.32.11"
	}
}
